<script type="text/javascript" src="@Url.Content("~/Scripts/jquery-ui-1.8.24.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery-ui-1.8.24.min.js")"></script>

<!-- Plugins - Via CDN -->
<script type="text/javascript" src="~/Scripts/jquery.flot.min.js"></script>
<script type="text/javascript" src="~/Scripts/jquery.sparkline.min.js"></script>
<script type="text/javascript" src="~/Scripts/jquery.dataTables.min.js"></script>
<script type="text/javascript" src="~/Scripts/fullcalendar.min.js"></script>
<script type="text/javascript" src="~/Scripts/bootstrap.js"></script>


<script type="text/javascript" src="~/Scripts/jquery.scrollTo-1.4.3.1-min.js"></script>

<!-- Theme Javascript -->
<script type="text/javascript" src="~/Scripts/uniform.min.js"></script>
<script type="text/javascript" src="~/Scripts/main.js"></script>
<script type="text/javascript" src="~/Scripts/custom.js"></script>
<script type="text/javascript" src="~/Scripts/sb-admin-2.js"></script>
<script type="text/javascript" src="~/vendor/plugins/metisMenu/metisMenu.min.js"></script>

<script src="@Url.Content("~/Scripts/jquery.tablesorter.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.tablesorter.pager.js")" type="text/javascript"></script>

<script src="@Url.Content("~/Scripts/dataTables.bootstrap.js")"></script>
<script src="@Url.Content("~/Scripts/jquery.dataTables.min.js")"></script>
<link href="http://cdn.datatables.net/plug-ins/a5734b29083/integration/bootstrap/3/dataTables.bootstrap.css" rel="stylesheet" />
<script src="http://cdn.datatables.net/plug-ins/a5734b29083/integration/bootstrap/3/dataTables.bootstrap.js"></script>
<script src="http://cdn.datatables.net/responsive/1.0.1/js/dataTables.responsive.js"></script>
<script type="text/javascript">
    jQuery(document).ready(function () {
        jQuery('#basicTable').DataTable({
            responsive: true,
        });
        console.clear();
    });
</script>

@Html.Partial("../Shared/Header")
<section>
    <div class="mainwrapper">
        <!-- Start: Sidebar -->
        <!-- End: Sidebar -->
        @Html.Partial("../Shared/LeftMenu")
        <!-- Start: Content -->
        <div class="mainpanel">
            <div class="col-lg-12 col-md-12">
                @if (Model.ClientLeadList != null && Model.ClientLeadList.Any())
                {
                    <div style="height: 15px"></div>
                    <label style="font-size: 16px;color:red">Client Lead Report</label>
                    <br />
                    <table id="basicTable" class="table table-striped table-hover table-condensed col-lg-8 col-md-8 col-sm-8">
                        <thead>
                            <tr>
                                <th>Sr.No.</th>
                                <th>Code</th>
                                <th>Name</th>
                                <th>Address</th>
                                <th>Contact</th>
                                <th>Country</th>
                                <th>State</th>
                                <th>City</th>
                                <th>District</th>
                                <th>Requriment</th>
                                <th>Date</th>
                                <th>ScheduleDate</th>
                                <th>Remark</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{ 
                                int rowid = 1;
                            }
                            @foreach (var data in Model.ClientLeadList)
                            {
                                <tr>
                                    <td>@rowid</td>
                                    <td>@data.ClientLeadCode</td>
                                    <td>@data.ClientName</td>
                                    <td>@data.Address</td>
                                    <td>@data.ContactNo1</td>
                                    <td>@data.Country</td>
                                    <td>@data.State</td>
                                    <td>@data.District</td>
                                    <td>@data.City</td>
                                    <td>@data.Requriment</td>
                                    <td>@data.Date</td>
                                    <td>@data.ScheduleDate</td>
                                    <td>@data.Remark</td>
                                    @{
                                        rowid += 1;
                                    }
                                </tr>
                            }
                        </tbody>
                    </table>
                                        }
                                        else
                                        {
                                            <label style="color: red; text-align: center; font-weight: bolder; width: 100%">Record Not Found..</label>
                                            <script>
                                                    document.getElementById("printbtn").style.display = 'none';
                                                    document.getElementById("pager").style.display = 'none';
                                            </script>
                                        }
            </div>

        </div>
    </div>
</section>