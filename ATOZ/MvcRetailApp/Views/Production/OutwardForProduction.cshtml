@*CALCULATE AMOUNT + CALCULATE AMOUNT USING DISCOUNT IN RS OR PERCENT*@
<script type="text/javascript">
    function CalculateAmount(count) {

        var checkcounter = 0;
        var quantity = "Quantity" + count;
        var stkqty = "StkQty" + count;
        var costprice = "CostPrice" + count;
        var sellingprice = "SellingPrice" + count;
        var amount = "Amount" + count;
        var amountvalue = "AmountVal" + count;

        var finalquantity = +document.getElementById(quantity).value;
        var finalstockquantity = +document.getElementById(stkqty).innerHTML;
        var finalcost = +document.getElementById(costprice).innerHTML;

        if (finalquantity > finalstockquantity) {
            checkcounter = 1;
            alertbox("Can Not Exceed Stock Quantity..");
            document.getElementById(quantity).value = "";
        }

        if (checkcounter == 0) {
            var finalamount = finalquantity * finalcost;
            document.getElementById(amount).innerHTML = finalamount.toFixed(2);
            document.getElementById(amountvalue).value = finalamount.toFixed(2);
            CalculateTotalAmount();
        }

    }
</script>

<script type="text/javascript">
    function CalculateTotalAmount() {
        var count = document.getElementById("hdnRowCount").value;
        var ttlamt = 0;
        for (i = 1; i <= count; i++) {
            var amount = "AmountVal" + i;
            if (document.getElementById(amount) != null) {
                var amtval = +document.getElementById(amount).value;
                ttlamt = ttlamt + amtval;
            }
        }
        document.getElementById("TotalAmount").innerHTML = ttlamt.toFixed(2);
        document.getElementById("TotalAmountVal").value = ttlamt;
        CalculateGrandTotal();
    }
</script>

