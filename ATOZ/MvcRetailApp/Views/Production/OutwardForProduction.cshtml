@*GET ITEM DETAILS ON ITEM DDL*@
<script type="text/javascript">
    function GetItemDetailsByItem(rowid) {

        //CHECK IF SAME BARCODE IS ALREADY SELECTED THEN ENABLES PREVIOUS ROW
        itemcode = "ItemCode" + rowid;
        barcode = "Barcode" + rowid;
        var rowcount = document.getElementById("hdnRowCount").value;
        var itemcode = document.getElementById(itemcode).value;
        var barcode = document.getElementById(barcode).value;
        var counter = 0;
        var validationcounter = 0;
        for (i = 1; i < rowcount; i++) {
            var BARCODE = "Barcode" + i;
            var ITEMCODE = "ItemCode" + i;
            var QUANTITY = "Quantity" + i;
            var STKQTY = "StkQty" + i;
            var SHOPGODOWNQTY = "ShopGodownQty" + i;
            var UNIT = "Unit" + i;
            var COSTPRICE = "CostPrice" + i;
            var COLOR = "Color" + i;
            var MATERIAL = "Material" + i;
            var AMOUNT = "Amount" + i;

            //IF ITEM IS ALREADY SELECTED THEN GET ALERT
            if (document.getElementById(ITEMCODE) != null) {
                if ($("#" + BARCODE).val().toUpperCase() == barcode.toUpperCase()) {
                    alertbox("The Item Is Already Selected Please Change The Quantity..");
                    document.getElementById(QUANTITY).disabled = false;
                    document.getElementById(ITEMCODE).disabled = false;

                    //ITEM IS ALREADY SELECTED THEN EMPTY CURRENT ROW
                    var BARCODE = "Barcode" + rowcount;
                    var ITEMCODE = "ItemCode" + rowcount;
                    var QUANTITY = "Quantity" + rowcount;
                    var STKQTY = "StkQty" + rowcount;
                    var SHOPGODOWNQTY = "ShopGodownQty" + rowcount;
                    var UNIT = "Unit" + rowcount;
                    var COSTPRICE = "CostPrice" + rowcount;
                    var COLOR = "Color" + rowcount;
                    var MATERIAL = "Material" + rowcount;
                    var AMOUNT = "Amount" + rowcount;

                    document.getElementById(BARCODE).value = "";
                    document.getElementById(ITEMCODE).value = "";
                    document.getElementById(QUANTITY).value = "";
                    document.getElementById(STKQTY).innerHTML = "";
                    document.getElementById(SHOPGODOWNQTY).innerHTML = "";
                    document.getElementById(UNIT).innerHTML = "";
                    document.getElementById(COSTPRICE).innerHTML = "";
                    document.getElementById(COLOR).innerHTML = "";
                    document.getElementById(MATERIAL).innerHTML = "";
                    document.getElementById(AMOUNT).innerHTML = "";

                    counter = i;
                    break;
                }
            }
        }

        //IF COUNTER IS 0 THAT MEANS CURRENT ITEM IS NOT SELECED IN PREVIOUS ROWS
        if (counter == 0) {
            //IF PREVIOUS ROW IS ENABLED FOR CHANGE THE QUANTITY BUT WE SELECT AGAIN DDL THEN DISABLED THIS ENABLES QUANTITY
            for (i = 1; i < rowcount; i++) {

                var BARCODE = "Barcode" + i;
                var ITEMCODE = "ItemCode" + i;
                var QUANTITY = "Quantity" + i;
                var STKQTY = "StkQty" + i;
                var SHOPGODOWNQTY = "ShopGodownQty" + i;
                var UNIT = "Unit" + i;
                var COSTPRICE = "CostPrice" + i;
                var COLOR = "Color" + i;
                var MATERIAL = "Material" + i;
                var AMOUNT = "Amount" + i;

                if ($("#" + BARCODE).val() == "") {
                    alertbox("Please Select The Barcode");
                    var BARCODE = "Barcode" + rowcount;
                    document.getElementById(BARCODE).value = "";
                    validationcounter++;
                }
                else if ($("#" + QUANTITY).val() == "") {
                    alertbox("Please Fill The Quantity");
                    var BARCODE = "Barcode" + rowcount;
                    document.getElementById(BARCODE).value = "";
                    validationcounter++;
                }
            }

            if (validationcounter == 0) {
                //DISPLAY DETAILS ON ITEM CHANGE EVENT
                var barcode = document.getElementById("Barcode" + rowid).value;
                var shopgodowncode = document.getElementById("loginshopgodowncode").value;
                if (barcode != "") {
                    document.getElementById("Quantity" + rowid).disabled = false;
                    $.getJSON("/Production/GetItemDetails", { barcode: barcode, shopgodown: shopgodowncode },
                          function (data) {
                              if (data != "Fail" && data != "No Stock") {

                                  document.getElementById("StkQty" + rowid).innerHTML = data.totalstkqty;
                                  document.getElementById("ShopGodownQty" + rowid).innerHTML = data.Quantity;
                                  document.getElementById("Unit" + rowid).innerHTML = data.Unit;
                                  document.getElementById("CostPrice" + rowid).innerHTML = +(data.SellingPrice).toFixed(2);
                                  document.getElementById("Color" + rowid).innerHTML = data.Color;
                                  document.getElementById("Material" + rowid).innerHTML = data.Material;
                                  document.getElementById("ProdItemVal" + rowid).value = document.getElementById("ProdItemCount").value;

                                  document.getElementById("BatchNo" + rowid).value = data.BatchNo;
                                  document.getElementById("UnitVal" + rowid).value = data.Unit;
                                  document.getElementById("NumberType" + rowid).value = data.NumberType;
                                  document.getElementById("ItemLabel" + rowid).innerHTML = data.ItemName;
                                  document.getElementById("ItemCode" + rowid).value = data.ItemCode;
                                  document.getElementById("ItemName" + rowid).value = data.ItemName;
                                  document.getElementById("ColorVal" + rowid).value = data.Color;
                                  document.getElementById("MaterialVal" + rowid).value = data.Material;
                                  document.getElementById("CostPriceVal" + rowid).value = +(data.SellingPrice).toFixed(2);
                                  document.getElementById("Size" + rowid).value = data.Size;
                                  document.getElementById("Brand" + rowid).value = data.Brand;
                                  document.getElementById("DesignCode" + rowid).value = data.DesignCode;
                                  document.getElementById("DesignName" + rowid).value = data.DesignName;
                                  document.getElementById("Description" + rowid).value = data.Description;
                                  document.getElementById("SellingPrice" + rowid).value = data.SellingPrice;
                                  document.getElementById("MRP" + rowid).value = data.MRP;
                              }
                              else if (data == "No Stock") {
                                  alertbox("No Stock Available");
                                  $("#Barcode" + rowid).val("");
                              }
                              else {
                                  alertbox("Item Not Present..");
                                  $("#Barcode" + rowid).val("");
                              }
                          });
                }
            }
        }
    }
</script>

